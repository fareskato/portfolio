<h1>Portfolio Items</h1>
<%= link_to 'Create New Portfolio', new_portfolio_path %>
<% @portfolio_items.each do | portfolio_item | %>
  <p><%= portfolio_item.title %></p>
  <p><%= portfolio_item.subtitle %></p>
  <p><%= portfolio_item.body %></p>
  <%= image_tag portfolio_item.thumb_image if portfolio_item.thumb_image != nil  %> <br>
  <%= link_to 'Edit Portfolio', edit_portfolio_path(portfolio_item.id) %>
<% end %>
